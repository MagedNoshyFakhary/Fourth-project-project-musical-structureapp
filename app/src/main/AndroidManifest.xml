<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.maged.elmagdmusic">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.TracksActivity"
                android:label="@string/TracksActivity"
                android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Activities.ArtistActivity"
            android:label="@string/ArtistActivity"
            android:theme="@style/AppTheme"
            android:parentActivityName="Activities.ArtistActivity"/>

        <activity android:name=".Activities.PlayActivity"
            android:label="@string/PlayActivity"
            android:theme="@style/AppTheme"
            android:parentActivityName="Activities.PlayActivity" />

        <activity android:name=".Activities.AlbumActivity"
            android:label="@string/AlbumActivity"
            android:theme="@style/AppTheme"
            android:parentActivityName="Activities.AlbumActivity"/>
    </application>

</manifest>